<!DOCTYPE html>
<html>
  <head>
    <title>MoveIndex</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <% if current_user %>
      <strong><%= current_user.email %></strong>
      <%= link_to t('devise.sessions.sign_out'), destroy_user_session_path, method: "delete" %>
    <% else %>
      <%= link_to t('devise.registrations.sign_up'), new_user_registration_path %>
      <%= link_to t('devise.sessions.sign_in'), new_user_session_path %>
    <% end %>
    <%= yield %>
  </body>
</html>
